{
  "name": "color-microservice",
  "description": "Get predominant colors from an URL image",
  "homepage": "https://documentup.com/Kikobeats/color-microservice",
  "version": "0.0.0",
  "main": "src/index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikobeats/color-microservice.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/color-microservice/issues"
  },
  "keywords": [
    "microservice",
    "color",
    "dominant",
    "predominant"
  ],
  "dependencies": {
    "body-parser": "~1.18.2",
    "colorable-dominant": "~1.1.0",
    "compression": "~1.7.0",
    "cors": "~2.8.4",
    "express": "~4.16.2",
    "express-status-monitor": "~1.1.0",
    "helmet": "~3.12.0",
    "jsendp": "~2.0.0",
    "morgan": "~1.9.0",
    "splashy": "~2.0.1"
  },
  "devDependencies": {
    "coveralls": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "svr": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dev": "svr src/index.js",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "precommit-lint": "prettier-standard",
    "prelint": "npm run pretty",
    "pretest": "npm run lint",
    "pretty": "prettier-standard {core,test,bin}/**/*.js",
    "start": "node src/server.js",
    "test": "exit 0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "precommit-lint",
      "git add"
    ]
